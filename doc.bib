@inbook{10.1145/3724504.3724512,
author = {Lei, Bin and Zhang, Zixuan and Chen, Minghui and Li, Junhao and Deng, Yukun and Chen, Shurong and Zhang, Hui and Su, Jiang},
title = {Design and Implementation of Intelligent Production Line Equipment for Mechanical and Electrical Training Teaching Based on Vacuum Adsorption Technology},
year = {2025},
isbn = {9798400711732},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3724504.3724512},
abstract = {In response to the problems of insufficient training resources, adjustable training equipment, and disconnection between training content and teaching content in the current electromechanical professional training and teaching equipment, this article designs a mechatronics integrated training and teaching production line equipment based on vacuum adsorption technology. This equipment is based on motor motion control technology, combined with practical work scenarios and practical teaching content design. Four sets of practical teaching modules have been designed, corresponding to the technical skills of three-phase asynchronous motors, DC motors, stepper motors, and servo motors. By designing a semi open electrical control module, students can personally participate in the electrical assembly and debugging process of the equipment modules. The production line equipment adopts an integrated Siemens PLC as the core controller, various motors as execution components, and combines digital twin technology to design an automatic production line equipment for electromechanical training and teaching. Through this device, students can gain a deeper understanding of the application scenarios and motion control methods of four types of motors, effectively improving the practical teaching effectiveness of the teaching equipment.},
booktitle = {Proceedings of the 2024 2nd International Conference on Information Education and Artificial Intelligence},
pages = {40–50},
numpages = {11}
}

