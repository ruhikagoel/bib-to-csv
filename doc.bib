@inproceedings{10.1145/3723178.3723185,
author = {Sohan, Md. Faruk Abdullah Al and Ahmed, Mustakim and Redwan, Kazi},
title = {Comprehensive Review of Post-Vaccination Side Effects in COVID-19 Vaccines: Incidence and Severity},
year = {2025},
isbn = {9798400713828},
publisher = {Association for Computing Machinery},
