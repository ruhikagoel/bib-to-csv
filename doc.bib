series = {PCI '24}
}

@inproceedings{10.1145/3727505.3727506,
author = {Wu, Guangfu and Liu, Yang},
title = {Research on Spam Message Identification Based on Big Data Summary: Spam message detection and analyses Design and implementation of accurate spam identification and filtering system based on big data technology},
year = {2025},
isbn = {9798400713620},
publisher = {Association for Computing Machinery},
