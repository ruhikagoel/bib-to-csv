@inbook{10.5555/3712729.3712937,
author = {N\'{u}\~{n}ez-Chongo, Osiris and Su\'{a}rez-Dur\'{a}n, Mauricio and Asorey, Hern\'{a}n and Sidelnik, Iv\'{a}n and Mayo-Garc\'{\i}a, Rafael and M\'{e}ndez, Roberto and Carretero, Manuel},
title = {Advancing Neutron Safety and Dosimetry in Nuclear Facilities: Applications and Current Status of the Development of Nereida},
year = {2025},
isbn = {9798331534202},
publisher = {IEEE Press},
abstract = {The development of nuclear facilities necessitates reliable tools for design, licensing, and safety assessments. NEREIDA (the Spanish acronym for fast neutrons for the exploitation of facilities with atomic devices) is being developed for providing a robust solution. Utilizing Geant4-based Monte Carlo methods, NEREIDA characterizes neutron flux spectra and calculates dosimetric quantities such as environmental and personal dose equivalents in any neutron-production facility. This innovative tool integrates state-of-the-art codes for neutron transport, interaction, moderation, and material activation. Our preliminary results, obtained after the first year of a three-year development period, demonstrate computational scalability across various high-performance computing clusters and clouds. NEREIDA will generate the Geant4 facility's models from CAD files, structural materials, and neutron source characteristics without requiring programming expertise. Additionally, NEREIDA models the impact of the cosmic ray background and includes anthropomorphic phantoms for comprehensive safety assessments. This work discusses NEREIDA's current status and future directions, emphasizing its critical role in enhancing nuclear facility safety.},
booktitle = {Proceedings of the Winter Simulation Conference},
pages = {2511–2522},
numpages = {12}
}

@inproceedings{10.14236/ewic/BCSHCI2024.16,
author = {Petrie, Helen and Sreekumar, Gayathri},
title = {Passwords and single sign-on: Use, security, and understanding for online accounts},
year = {2025},
publisher = {BCS Learning &amp; Development Ltd},
address = {Swindon, GBR},
url = {https://doi.org/10.14236/ewic/BCSHCI2024.16},
doi = {10.14236/ewic/BCSHCI2024.16},
abstract = {As services move online, the number of password-protected accounts grows, and thus potentially the number of passwords users need, which should all be unique and strong. Single Sign-On (SSO) provides a secure alternative authentication method, but research has shown that users have security concerns about this method and over-estimate the amount of information which it passes between online services. However, much of that research is a decade old. This study investigated the use of passwords and contrast that with the use of SSO and understanding of and attitudes to SSO, using an online questionnaire with 98 UK participants. It found that participants have many password-protected accounts (median 30) and log into them on average 8 times a day. But their password habits are insecure in a number of ways, they do not sign out of online accounts regularly and re-use passwords either exactly or with small changes. Three-quarters of participants use SSO, for a range of online account types, banking being the only exception. Users of SSO find it easy and quick, and it relieves the burden of remembering and entering passwords. However, both users and non-users of SSO rate it as moderately high risk and have numerous security concerns about it. They also appear to have poor mental models of how it works and over-estimate what information from their account is shared. These results suggest that users need more information and education about the security of passwords and SSO.},
booktitle = {Proceedings of the 37th International BCS Human-Computer Interaction Conference},
pages = {172–181},
numpages = {10},
keywords = {Online authentication procedures, Single Sign-On, Passwords, Users’ understanding},
location = {University of Central Lancashire (UCLan)},
series = {BCS HCI '24}
}

