@inproceedings{10.1145/3728199.3728275,
author = {Lu, Kuang and Song, Xia},
title = {Research on Intelligent Monitoring System for Special Vehicles Based on Cloud Computing and Internet of Things},
year = {2025},
isbn = {9798400713231},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3728199.3728275},
doi = {10.1145/3728199.3728275},
abstract = {As cloud computing and Internet of Things (IoT) technologies continue to advance rapidly, there is a growing focus on intelligent monitoring systems for special vehicles. This paper investigates the design and implementation of an intelligent monitoring system that leverages cloud computing and IoT to enhance the management efficiency and safety of these vehicles. By combining the robust storage and processing capabilities of cloud computing with the extensive sensing and real-time communication features of IoT, a comprehensive monitoring platform is developed. The system utilizes a variety of sensors and network devices to gather real-time data on operational status, location, and environmental conditions of special vehicles, which is then transmitted to the cloud for analysis and processing. The system not only supports real-time monitoring, data storage and historical query functions, but also uses intelligent algorithms to realize anomaly detection, early warning and decision support, which effectively improves the safety management level of special vehicles. The research shows that the intelligent monitoring system for special vehicles based on cloud computing and Internet of things can effectively solve the problems of information island and data processing lag in traditional monitoring methods, and improve the timeliness and accuracy of monitoring. Through the actual case analysis, the system has achieved good application results in special vehicle scheduling, maintenance, fault warning and other aspects, and has important promotion value. This paper offers a theoretical foundation and practical insights for the optimized development of intelligent monitoring systems for special vehicles.},
booktitle = {Proceedings of the 2025 3rd International Conference on Communication Networks and Machine Learning},
pages = {456–462},
numpages = {7},
keywords = {Cloud Computing, Data Analysis, Intelligent Monitoring System, Internet of Things, Special Vehicles},
location = {
},
series = {CNML '25}
}

@article{10.1145/3729418,
author = {Hanisch, Simon and Arias-Cabarcos, Patricia and Parra-Arnau, Javier and Strufe, Thorsten},
title = {Anonymization Techniques for Behavioral Biometric Data: A Survey},
year = {2025},
issue_date = {November 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {57},
number = {11},
issn = {0360-0300},
url = {https://doi.org/10.1145/3729418},
doi = {10.1145/3729418},
abstract = {Our behavior—the way we talk, walk, act, or think—is unique and can be used as a biometric trait. It also correlates with sensitive attributes such as emotions and health conditions. With more and more behavior tracking techniques (e.g., fitness trackers, mixed reality) entering our everyday lives, more of our behavior is captured and processed. Hence, techniques to protect individuals’ privacy against unwanted inferences are required before such data is processed. To consolidate knowledge in this area, we are the first to systematically review suggested anonymization techniques for behavioral biometric data. We taxonomize and compare existing solutions regarding privacy goals, conceptual operation, advantages, and limitations. Our categorization allows for the comparison of anonymization techniques across different behavioral biometric traits. We review anonymization techniques for the behavioral biometric traits of voice, gait, hand motions, eye gaze, heartbeat (ECG), and brain activity (EEG). Our analysis shows that some behavioral traits (e.g., voice) have received much attention, while others (e.g., eye gaze, brain activity) are mostly neglected. We also find that the evaluation methodology of behavioral anonymization techniques can be further improved.},
journal = {ACM Comput. Surv.},
month = jun,
articleno = {272},
numpages = {54},
keywords = {Privacy, behavioral data, de-identification}
}

