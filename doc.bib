@inproceedings{10.1145/3734477.3734713,
author = {Ishtiaq, Abdullah Al and Anwar, Raja Hasnain and Chandio, Yasra and Anwar, Fatima Muhammad and Hussain, Syed Rafiul and Raza, Muhammad Taqi},
title = {Cloud Nine Connectivity: Security Analysis of In-Flight Wi-Fi Paywall Systems},
year = {2025},
isbn = {9798400715303},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3734477.3734713},
doi = {10.1145/3734477.3734713},
abstract = {In-flight Wi-Fi provides high-speed Internet connectivity to travelers at 30,000 feet at premium fees. In this paper, we present the first systematic study of the architecture and security policies of in-flight Wi-Fi paywall systems using network tomography analysis. We discover that attackers can exploit the inherent architectural shortcomings of airborne networks to create covert channels and conceal data packets within certain ''always-allowed'' traffic for free Internet access. Moreover, broken device authentication policies in these systems allow unlimited complimentary Internet connectivity. Finally, insecure ARP policies allow attackers to steal paid users' bandwidth to access the free Internet even faster. We validate these issues in practice over two major in-flight Wi-Fi providers using common protocols, e.g., UDP, DNS, etc. We also find that the root causes of these issues stem from different design choices in the architectures of these systems and propose countermeasures to address these flaws and prevent similar attacks.},
booktitle = {18th ACM Conference on Security and Privacy in Wireless and Mobile Networks},
pages = {76–87},
numpages = {12},
keywords = {in-flight wi-fi, paywall bypass, paywall systems},
location = {Arlington, VA, USA},
series = {WiSec 2025}
}

@inproceedings{10.1145/3723178.3723201,
author = {Ahmed, Shaikh Navid and Sajib, Md. Sajedul Islam and Ahmed, Naim and Ali, Umme Sumaya and Fariha, Laila Tuz Joha and Afroz, Amina and Shibly, Mir Moynuddin Ahmed and Ahsan, Mohammad Shamim},
title = {AI and RBAC Enhanced Security System for Hospitals in Bangladesh},
year = {2025},
isbn = {9798400713828},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3723178.3723201},
doi = {10.1145/3723178.3723201},
abstract = {The security landscape of hospitals in Bangladesh, like many healthcare facilities globally, is facing escalating challenges. Outdated security measures contribute to vulnerabilities, commercial losses, reputational damage, and legal consequences for healthcare institutions. To mitigate these crucial security concerns, there is a growing need to integrate advanced technologies like AI into hospital security systems. Keeping this in mind, in this paper, we propose a novel system based on Artificial Intelligence (AI) and Role-Based Access Control (RBAC) to ensure security in Bangladeshi hospitals. Since traditional methods like guards and paper records are insufficient, the integration of advanced technologies like AI and RBAC offers a solution to hospital security systems by enhancing the monitoring and threat detection capabilities. Specifically, we introduce a constrained RBAC model that limits access to authorized personnel ensuring patient data privacy. Besides, we examine the security challenges faced by the hospitals and the probable risks such as unauthorized access, child abduction, and potential data breaches. Furthermore, our proposed system achieves 91% precision and 85% accuracy in the identification of authorized personnel, which validates the correctness and effectiveness of the model.},
booktitle = {Proceedings of the 3rd International Conference on Computing Advancements},
pages = {169–176},
numpages = {8},
keywords = {Artificial Intelligence, Image-processing, Constrained RBAC, Hospital Security System, Data Breach.},
location = {
},
series = {ICCA '24}
}

@inproceedings{10.1145/3706599.3707399,
author = {Lin, Spencer and Jun, Miru and Rizk, Basem and Shieh, Karen and Fisher, Scott S. and Mozgai, Sharon A.},
title = {Optimizing SIA Development: A Case Study in User-Centered Design for Estuary, a Multimodal Socially Interactive Agent Framework},
