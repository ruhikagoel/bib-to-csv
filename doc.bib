@inproceedings{10.1145/3705754.3705793,
author = {Song, Qi and Liu, Jia and Zhang, Caipu and Wang, Zhen and Wei, Zhenkun and Ma, Pengcheng},
title = {Classification of IPSec VPN Encrypted Traffic Based on a Hybrid Method},
year = {2025},
isbn = {9798400710193},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3705754.3705793},
doi = {10.1145/3705754.3705793},
abstract = {Internet traffic classification has gained significance with the rapid growth of Internet networks and online applications. In this paper, we propose the IPSO-AlexNet-LSTM model, which effectively distinguishes encrypted network traffic. By combining the payload features and time-flow features of packets into feature vectors, AlexNet is used to extract the static features of IPSec VPN packets, and then the effectiveness of Long Short-Term Memory (LSTM) networks is utilized to capture the temporal dependencies of the packets. To further optimize model performance, an improved Particle Swarm Optimization (PSO) heuristic algorithm is employed to fine-tune the hyperparameters of LSTM, ensuring enhanced robustness and generalization when handling the random and dynamic behaviors of malicious attacks. Finally, the experimental results demonstrate that the proposed approach successfully identifies IPSec VPN application traffic types and achieves a balanced trade-off between classification accuracy and speed, enabling efficient recognition of IPSec VPN encrypted traffic.},
booktitle = {Proceedings of the 2024 2nd International Conference on Electronics, Computers and Communication Technology},
pages = {204–209},
numpages = {6},
keywords = {Encrypted traffic classification, IPSec VPN, LSTM, Parameter optimization},
location = {
},
series = {CECCT '24}
}

@inproceedings{10.1145/3715336.3735758,
author = {Yoo, Dong Whi and Stroud, Austin M. and Zhu, Xuan and Miller, Jennifer E. and Barry, Barbara},
title = {Toward Patient-Centered AI Fact Labels: Leveraging Extrinsic Trust Cues},
year = {2025},
isbn = {9798400714856},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3715336.3735758},
doi = {10.1145/3715336.3735758},
abstract = {AI technologies in healthcare hold great promise for addressing numerous challenges, but ensuring that patients understand, trust, and adopt these technologies remains a significant hurdle. While the HCI community has proposed AI documentation frameworks (e.g., model cards) to enhance understanding, patient perspectives in the healthcare AI documentation remain underexplored. To address this gap, we designed prototypes based on existing frameworks and gathered feedback from 18 participants to explore their perspectives on AI documentation in cardiology, a domain where high-stakes AI tools are increasingly used and understanding users’ trust in AI is essential. Our findings revealed patient needs for more detailed information about healthcare AI technologies, the importance of extrinsic trust cues (e.g., regulatory status), and the integration of AI documentation into existing care processes. Based on these findings, we discuss two design implications: enhancing patient-centeredness in AI documentation and leveraging extrinsic trust cues to improve its design. This study contributes to the HCI community by amplifying the patient voice in designing AI documentation and offering actionable insights into leveraging extrinsic trust cues effectively.},
booktitle = {Proceedings of the 2025 ACM Designing Interactive Systems Conference},
pages = {676–690},
numpages = {15},
keywords = {Patient, AI Facts Label, AI Documentation, Trust},
location = {
},
series = {DIS '25}
}

@article{10.1145/3733838,
author = {Cinca, Robert and Costanza, Enrico and Musolesi, Mirco},
title = {Practitioners and Bias in Machine Learning: A Study},
year = {2025},
issue_date = {June 2025},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {15},
number = {2},
issn = {2160-6455},
url = {https://doi.org/10.1145/3733838},
doi = {10.1145/3733838},
abstract = {The increasing adoption of machine learning (ML) raises ethical concerns, particularly regarding bias. This study explores how ML practitioners with limited experience in bias understand and apply bias definitions, detection measures, and mitigation methods. Through a take-home task, exercises, and interviews with 22 participants, we identified five key themes: sources of bias, selecting bias metrics, detecting bias, mitigating bias, and ethical considerations. Participants faced unresolved conflicts, such as applying fairness definitions in practice, selecting context-dependent bias metrics, addressing real-world biases, balancing model performance with bias mitigation, and relying on personal perspectives over data-driven metrics. While bias mitigation techniques helped identify biases in two datasets, participants could not fully eliminate bias, citing the oversimplification of complex processes into models with limited variables. We propose designing bias detection tools that encourage practitioners to focus on the underlying assumptions and integrating bias concepts into ML practices, such as using a harmonic mean-based approach, akin to the F1 score, to balance bias and accuracy.},
journal = {ACM Trans. Interact. Intell. Syst.},
month = jun,
articleno = {12},
numpages = {28},
keywords = {ML Bias, Operationalizing Bias, machine learning, machine learning practitioners, interview study}
}

