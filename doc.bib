@inproceedings{10.1145/3733965.3733972,
author = {Liu, Haiyun and Xue, Jiahao and Zhao, Shangqing and Liu, Yao and Lu, Zhuo},
title = {The Dual Role of Large Language Models in Network Security: Survey and Research Trends},
year = {2025},
isbn = {9798400715310},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3733965.3733972},
doi = {10.1145/3733965.3733972},
abstract = {Large language models (LLMs) have profoundly shaped various domains, including several types of network systems. With their powerful capabilities, LLMs have recently been proposed to enhance network security. However, the development of LLMs can introduce new risks due to their potential vulnerabilities and misuse. In this paper, we are motivated to review the dual role of LLMs in network security. Our goal is to explore how LLMs impact network security and ultimately shed light on how to evaluate LLMs from a network security perspective. We further discuss several future research directions regarding how to scientifically enable LLMs to assist with network security.},
booktitle = {Proceedings of the 2025 ACM Workshop on Wireless Security and Machine Learning},
pages = {20–25},
numpages = {6},
keywords = {large language model (llm), network security, adversarial attacks},
location = {USA},
series = {WiseML '25}
}

@inproceedings{10.1145/3729706.3729780,
author = {Wang, Yu and Wang, Haijun and Yao, Yingyi and Jiao, Zongshuang and Zhou, Linlin and Wu, Yufei},
title = {Intelligent Connected Vehicle Data Security Protection Scheme Based on Federated Learning and Blockchain},
year = {2025},
isbn = {9798400712715},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3729706.3729780},
doi = {10.1145/3729706.3729780},
abstract = {While Intelligent Connected Vehicles (ICV) improve travel convenience, they also bring new security risks, especially how to achieve data security protection for the Internet of Vehicles (IoV), which has become a key bottleneck restricting the development of ICV and has received widespread attention from academic and industrial circles at home and abroad. Based on a comprehensive analysis of the data security risks faced by ICV, this article conducts a comprehensive comparative study of relevant solutions at home and abroad, and proposes an ICV data security protection model framework based on Federated Learning (FL) and blockchain technology. FL is used to solve the privacy leakage risk caused by raw data sharing in traditional architecture of IoV, while differential privacy technology is used to protect model parameters, and blockchain technology is used to address risks such as single point failures in FL. This framework provides a new idea for data security protection for ICV.},
booktitle = {Proceedings of the 2025 4th International Conference on Cyber Security, Artificial Intelligence and the Digital Economy},
pages = {466–470},
numpages = {5},
keywords = {Blockchain, Data Security, Federated Learning, Intelligent Connected Vehicles, Internet of Vehicles},
location = {
},
series = {CSAIDE '25}
}

@inproceedings{10.5555/3721488.3721833,
author = {T\"{u}rkay, Selen and Wozniak, Maciej K. and LeMasurier, Gregory and Caldwell, Glenda and Vermeulen, Jasper and Burden, Alan G.},
