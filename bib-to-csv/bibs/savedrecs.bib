@article{ WOS:001504060000020,
Author = {Zhu, Xiuzhen and Lin, Limei and Huang, Yanze and Wang, Xiaoding and
   Hsieh, Sun-Yuan and Wu, Jie},
Title = {Forward Legal Anonymous Group Pairing-Onion Routing for Mobile
   Opportunistic Networks},
