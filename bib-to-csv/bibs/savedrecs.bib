@inproceedings{ WOS:000485139900033,
Author = {Poddebniak, Damian and Dresen, Christian and Mueller, Jens and Ising,
   Fabian and Schinzel, Sebastian and Friedberger, Simon and Somorovsky,
   Juraj and Schwenk, Joerg},
