@inproceedings{ WOS:001159622900020,
Author = {Canham, Matthew},
Editor = {Degen, H and Ntoa, S and Moallem, A},
Title = {Repeat Clicking: A Lack of Awareness is Not the Problem},
Booktitle = {HCI INTERNATIONAL 2023 LATE BREAKING PAPERS, HCII 2023, PT VI},
