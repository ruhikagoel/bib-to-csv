Author = {Kwok, Thomas and Nguyen, Thao and Lam, Linh and Chieu, Trieu},
Editor = {Cheung, SC and Li, YS and Chao, KM and Younas, M and Chung, JY},
Title = {A Web-based and email driven electronic contract management system},
Booktitle = {ICEBE 2007: IEEE INTERNATIONAL CONFERENCE ON E-BUSINESS ENGINEERING,
   PROCEEDINGS},
Series = {International Conference on e-Business Engineering},
Year = {2007},
