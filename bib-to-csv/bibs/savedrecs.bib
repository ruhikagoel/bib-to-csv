@inproceedings{ WOS:000698688000019,
Author = {Nicholson, James and Coventry, Lynne and Briggs, Pam},
Book-Group-Author = {USENIX Assoc},
Title = {Can We Fight Social Engineering Attacks By Social Means? Assessing
   Social Salience as a Means to Improve Phish Detection},
Booktitle = {PROCEEDINGS OF THIRTEENTH SYMPOSIUM ON USABLE PRIVACY AND SECURITY
   (SOUPS 2017)},
Year = {2017},
Pages = {285-298},
Note = {13th Symposium on Usable Privacy and Security (SOUPS), Santa Clara, CA,
   JUL 12-14, 2017},
Organization = {USENIX Assoc; NSF; Facebook; Google; Microsoft; NetApp; ACM Queue;
   Distributed Management Task Force},
Abstract = {Phishing continues to be a problem for both individuals and
   organisations, with billions of dollars lost every year. We propose the
   use of nudges - more specifically social saliency nudges - that aim to
   highlight important information to the user when evaluating emails. We
   used Signal Detection Theory to assess the effects of both sender
   saliency (highlighting important fields from the sender) and receiver
   saliency (showing numbers of other users in receipt of the same email).
   Sender saliency improved phish detection but did not introduce any
   unwanted response bias. Users were asked to rate their confidence in
   their own judgements and these confidence scores were poorly calibrated
   with actual performance, particularly for phishing (as opposed to
   genuine) emails. We also examined the role of impulsive behaviour on
   phish detection, concluding that those who score highly on dysfunctional
   impulsivity are less likely to detect the presence of phishing emails.},
