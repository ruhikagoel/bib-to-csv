@inproceedings{ WOS:000527571900023,
Author = {Habib, Hana and Zou, Yixin and Jannu, Aditi and Sridhar, Neha and
   Swoopes, Chelse and Acquisti, Alessandro and Cranor, Lorrie Faith and
   Sadeh, Norman and Schaub, Florian},
