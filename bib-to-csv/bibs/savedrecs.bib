   Bello, Abubakar and Maurushat, Alana and Dale, John},
Editor = {Bernhard, M and Bracciali, A and Camp, LJ and Matsuo, S and Maurushat, A and Ronne, PB and Sala, M},
Title = {Incorporating Psychology into Cyber Security Education: A Pedagogical
   Approach},
Booktitle = {FINANCIAL CRYPTOGRAPHY AND DATA SECURITY, FC 2020},
