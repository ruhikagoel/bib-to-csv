@inproceedings{ WOS:000263225400017,
Author = {Pavaday, Narainsamy and Soyjaudah, K. M. S.},
Editor = {Jmaiel, M and Mosbah, M},
Title = {A comparative study of secret code variants in terms of keystroke
   dynamics},
Booktitle = {CRISIS: 2008 THIRD INTERNATIONAL CONFERENCE ON RISKS AND SECURITY OF
   INTERNET AND SYSTEMS, PROCEEDINGS},
