@article{ WOS:000171320800008,
Author = {D'Allesandro, DM and Dosa, NP},
Title = {Empowering children and families with information technology},
Journal = {ARCHIVES OF PEDIATRICS \& ADOLESCENT MEDICINE},
Year = {2001},
Volume = {155},
