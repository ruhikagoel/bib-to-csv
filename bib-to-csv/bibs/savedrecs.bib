@article{ WOS:001210719500003,
Author = {Pillai, Beena G. and Dayanand, N. Lal and Prasanna, T. Venkatesh},
Title = {Blockchain broadcast proxy ReEncryption in cloud environment for secure
   data sharing},
Journal = {SADHANA-ACADEMY PROCEEDINGS IN ENGINEERING SCIENCES},
Year = {2024},
Volume = {49},
Number = {2},
Month = {APR 25},
Abstract = {The conditional proxy broadcast ReEncryption mechanism enables the user
   to periodically re-encrypt the cipher-text. When a sender generates the
   key, ReEncryption id, which reuses for the encrypted data or the
   ciphertext. Two methods that only allow one recipient at a time are
   proxy ReEncryption and identity-based proxy ReEncryption. If there are
   multiple receivers, these systems need to be called numerous times,
   which is a significant drawback. To overcome this problem of a single
   receiver, proposed a new scheme, blockchain-based broadcast proxy
   ReEncryption (BBPRE), which allows the system to handle multiple
   receivers at a time. This BBPRE works similar to the proxy ReEncryption
   and identity-based proxy ReEncryption but allows multiple receivers and
   adds the time property. In this broadcast proxy ReEncryption, a
   sender/user can able to send ciphertext or encrypted to multiple
   receivers at a time, and all the receivers also have an encryption key
   provided by the sender/user.The receivers can able to re-encrypt this
   encrypted data or the ciphertext. It provides security to the user files
   during sending and sharing through electronic mail. The time stamp
   authority creates a public key, each period that has the most recent
   timestamp for every user.},
