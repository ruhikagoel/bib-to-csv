Author = {Florez Z, Jimmy and Logreira R, Camilo and Munoz, Mario and Francisco
   Vargas, Jesus},
Editor = {Claycomb, WR},
Title = {ARCHITECTURE OF INSTANT MESSAGING SYSTEMS FOR SECURE DATA TRANSMISSION},
