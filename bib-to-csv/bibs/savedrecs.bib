Title = {Making Sense of Large Volumes of Unstructured Email Responses},
Booktitle = {2016 SIXTEENTH INTERNATIONAL CONFERENCE ON ADVANCES IN ICT FOR EMERGING
   REGIONS (ICTER) - 2016},
Series = {International Conference on Advances in ICT for Emerging Regions},
Year = {2016},
