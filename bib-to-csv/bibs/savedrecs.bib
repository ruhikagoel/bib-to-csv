Title = {Vehicle Security and Management System on GPS Assisted Vehicle Using
   Geofence and Google Map},
Booktitle = {PROCEEDINGS OF SECOND INTERNATIONAL CONFERENCE ON ELECTRICAL SYSTEMS,
