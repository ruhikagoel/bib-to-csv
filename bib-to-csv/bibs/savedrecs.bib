@article{ WOS:001001672100001,
Author = {Mehus, Christopher J. and Stevenson, Brittany and Weiler, Lindsey and
   Gunlicks-Stoessel, Meredith and Morrell, Nicole and Patrick, Megan E.},
Title = {An example of implementing a safety protocol in remote intervention and
   survey research with college students},
Journal = {CLINICAL TRIALS},
