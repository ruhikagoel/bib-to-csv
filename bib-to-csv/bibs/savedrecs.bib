@article{ WOS:001377296800041,
Author = {Muniyandi, Amutha Prabakar and Balusamy, Balamurugan and Dhanaraj,
   Rajesh Kumar and Sumathi, D. and Nandakumar, S. and Preetha, K. S. and
   Alroobaea, Roobaea and Paramasivam, A.},
Title = {Intelligent Security System for Preventing DDoS Attacks for 6G Enabled
   WBSN Using Improve Grey Wolf Optimization},
Journal = {IEEE TRANSACTIONS ON CONSUMER ELECTRONICS},
Year = {2024},
Volume = {70},
