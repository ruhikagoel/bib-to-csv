@inproceedings{ WOS:000722005800005,
Author = {Stock, Ben and Pellegrino, Giancarlo and Li, Frank and Backes, Michael
   and Rossow, Christian},
Book-Group-Author = {Internet Soc},
Title = {Didn't You Hear Me? - Towards More Successful Web Vulnerability
   Notifications},
