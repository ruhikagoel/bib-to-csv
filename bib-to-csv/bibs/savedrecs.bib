@article{ WOS:001192590000001,
Author = {Carvalhais, Carlos and Dias, Ricardo and Costa, Carla and Silva, Manuela
   V.},
Title = {General Knowledge and Attitudes about Safety and Emergency Evacuation:
   The Case of a Higher Education Institution},
