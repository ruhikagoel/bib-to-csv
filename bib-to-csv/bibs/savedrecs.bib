@article{ WOS:001463492900003,
Author = {Li, Zhetao and Xiao, Yong and Liu, Haolin and Liao, Xiaofei and Yuan, Ye
   and Du, Junzhao},
Title = {Dynamic Graph Publication With Differential Privacy Guarantees for
   Decentralized Applications},
