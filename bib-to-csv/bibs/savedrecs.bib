@inproceedings{ WOS:000655496500011,
Author = {Sandoval, Itzel Vazquez and Lenzini, Gabriele},
Editor = {Saracino, A and Mori, P},
Title = {A Formal Security Analysis of the p ≡ p Authentication
   Protocol for Decentralized Key Distribution and End-to-End Encrypted
   Email},
Booktitle = {EMERGING TECHNOLOGIES FOR AUTHORIZATION AND AUTHENTICATION, ETAA 2019},
Series = {Lecture Notes in Computer Science},
Year = {2020},
Volume = {11967},
Pages = {171-187},
Note = {2nd International Workshop on Emerging Technologies for Authorization
   and Authentication (ETAA), Luxembourg, LUXEMBOURG, SEP 27, 2019},
Organization = {Collaborat \& Confidential Informat Sharing \& Anal Cyber Protect;
   European Network Cyber Secur; Marie Sklodowska Curie Act; Innovat
   Training Networks},
Abstract = {To send encrypted emails, users typically need to create and exchange
   keys which later should be manually authenticated, for instance, by
   comparing long strings of characters. These tasks are cumbersome for the
   average user. To make more accessible the use of encrypted email, a
   secure email application named p equivalent to p automates the key
   management operations; p equivalent to p still requires the users to
   carry out the verification, however, the authentication process is
   simple: users have to compare familiar words instead of strings of
   random characters, then the application shows the users what level of
   trust they have achieved via colored visual indicators. Yet, users may
   not execute the authentication ceremony as intended, p equivalent to p's
   trust rating may be wrongly assigned, or both. To learn whether p
   equivalent to p's trust ratings (and the corresponding visual
   indicators) are assigned consistently, we present a formal security
   analysis of p equivalent to p's authentication ceremony. From the
   software implementation in C, we derive the specifications of an
   abstract protocol for public key distribution, encryption and trust
   establishment; then, we model the protocol in a variant of the applied
   pi calculus and later formally verify and validate specific privacy and
   authentication properties. We also discuss alternative research
   directions that could enrich the analysis.},
Publisher = {SPRINGER INTERNATIONAL PUBLISHING AG},
Address = {GEWERBESTRASSE 11, CHAM, CH-6330, SWITZERLAND},
