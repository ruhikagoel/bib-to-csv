@article{ WOS:000261155700006,
Author = {Kolan, Prakash and Dantu, Ram and Cangussu, Joao W.},
Title = {Nuisance Level of a Voice Call},
Journal = {ACM TRANSACTIONS ON MULTIMEDIA COMPUTING COMMUNICATIONS AND APPLICATIONS},
