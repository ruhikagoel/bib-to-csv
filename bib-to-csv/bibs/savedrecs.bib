Title = {User and Device Tracking in Private Networks by Correlating Logs: A
   System for Responsive Forensic Analysis},
Booktitle = {2014 FOURTH INTERNATIONAL CONFERENCE ON COMMUNICATION SYSTEMS AND
   NETWORK TECHNOLOGIES (CSNT)},
Series = {International Conference on Communication Systems and Network
   Technologies},
