@inproceedings{ WOS:000540643200018,
Author = {Weidman, Jake and Grossklags, Jens},
Book-Group-Author = {ACM},
Title = {I Like It, but I Hate It: Employee Perceptions Towards an Institutional
   Transition to BYOD Second-Factor Authentication},
Booktitle = {33RD ANNUAL COMPUTER SECURITY APPLICATIONS CONFERENCE (ACSAC 2017)},
