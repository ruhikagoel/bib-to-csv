@inproceedings{ WOS:001037809500014,
Author = {Tally, Anne C. and Abbott, Jacob and Bochner, Ashley and Das, Sanchari
   and Nippert-Eng, Christena},
Book-Group-Author = {ACM},
Title = {Tips, Tricks, and Training: Supporting Anti-Phishing Awareness among
   Mid-Career Office Workers Based on Employees' Current Practices},
Booktitle = {PROCEEDINGS OF THE 2023 CHI CONFERENCE ON HUMAN FACTORS IN COMPUTING
   SYSTEMS, CHI 2023},
Year = {2023},
Note = {CHI conference on Human Factors in Computing Systems (CHI), Hamburg,
   GERMANY, APR 23-28, 2023},
Organization = {Assoc Comp Machinery; ACM SIGCHI; Google; Siemens; Bloomberg},
Abstract = {Preventing workplace phishing depends on the actions of every employee,
   regardless of cybersecurity expertise. Based on 24 semi-structured
   interviews with mid-career office workers (70.8\% women, averaging 44
   years old) at two U.S. universities, we found that less than 21\% of our
   participants had any formal anti-phishing training. Much of what our
   participants know about phishing comes from informal sources that
   emphasize ``tips{''} and ``tricks{''} like those found in conversations
   with friends, news stories, newsletters, social media, and podcasts.
   These informal channels provide opportunities for IT professionals
   wishing to enhance employees' anti-phishing awareness by better aligning
   the delivery of expert advice with employees' current practices and
   desires. We provide four recommendations designed to embrace ``guerrilla
   learning{''} by distributing anti-phishing educational resources across
   the workplace and workday in part to encourage the delivery of more
   accurate information in more informal and incidental ways, and greater
