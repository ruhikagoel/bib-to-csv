@inproceedings{ WOS:000256665200128,
Author = {Al-Hammadi, Yousof and Aickelin, Uwe},
Editor = {Jakoubi, S and Tjoa, S and Weippl, ER},
Title = {Detecting bots based on keylogging activities},
Booktitle = {ARES 2008: PROCEEDINGS OF THE THIRD INTERNATIONAL CONFERENCE ON
   AVAILABILITY, SECURITY AND RELIABILITY},
Year = {2008},
Pages = {896-902},
Note = {3rd International Conference on Availability, Reliability and Security,
   Barcelona, SPAIN, MAR 04-07, 2008},
Organization = {SECURE Business Austria; DEXA; Univ Politech Catalunya; Austrian Comp
   Soc; European \& Info Secur Agcy; IEEE Comp Soc},
Abstract = {A hot is a piece of software that is usually installed on an infected
   machine without the user's knowledge. A hot is controlled remotely by
   the attacker under a Command and Control structure. Recent statistics
   show that bots represent one of the fastest growing threats to our
   network by performing malicious activities such as email spamming or
   keylogging. However, few hot detection techniques have been developed to
   date. In this paper, we investigate a behavioural algorithm to detect a
   single hot that uses keylogging activity. Our approach involves the use
   of function calls analysis for the detection of the hot with a
   keylogging component. Correlation of the frequency of function calls
   made by the hot with other system signals during a specified time-window
   is performed to enhance the detection scheme. We perform a range of
   experiments with the spybot. Our results show that there is a high
   correlation between some function calls executed by this hot which
   indicates abnormal activity in our system.},
