@inproceedings{ WOS:000767230600010,
Author = {Siddique, Abid and Ghousuddin, Mohammed and Rehna, V. J.},
Book-Group-Author = {IEEE},
Title = {A Contemporary Approach Towards Curbing Cell Phone Use Related Road
   Mishaps},
Booktitle = {IECON 2021 - 47TH ANNUAL CONFERENCE OF THE IEEE INDUSTRIAL ELECTRONICS
   SOCIETY},
