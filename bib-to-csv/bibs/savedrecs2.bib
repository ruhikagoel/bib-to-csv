@article{ WOS:000561205700001,
Author = {Jampen, Daniel and Gur, Gurkan and Sutter, Thomas and Tellenbach,
   Bernhard},
Title = {Don't click: towards an effective anti-phishing training. A comparative
   literature review},
Journal = {HUMAN-CENTRIC COMPUTING AND INFORMATION SCIENCES},
Year = {2020},
Volume = {10},
Number = {1},
Month = {AUG 9},
Abstract = {Email is of critical importance as a communication channel for both
   business and personal matters. Unfortunately, it is also often exploited
   for phishing attacks. To defend against such threats, many organizations
   have begun to provide anti-phishing training programs to their
   employees. A central question in the development of such programs is how
   they can be designed sustainably and effectively to minimize the
   vulnerability of employees to phishing attacks. In this paper, we survey
   and categorize works that consider different elements of such programs
   via a clearly laid-out methodology, and identify key findings in the
   technical literature. Overall, we find that researchers agree on the
   answers to many relevant questions regarding the utility and
