@article{ WOS:001479443900009,
Author = {Wang, Changshuo and Gao, Xingyu and Wu, Meiqing and Lam, Siew-Kei and
   He, Shuting and Tiwari, Prayag},
Title = {Looking Clearer With Text: A Hierarchical Context Blending Network for
   Occluded Person Re-Identification},
