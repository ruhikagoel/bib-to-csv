@article{ WOS:001260149100008,
Author = {Barker, Nathan and Karlan, Dean and Udry, Christopher and Wright, Kelsey},
Title = {The Fading Treatment Effects of a Multifaceted Asset-Transfer Program in
   Ethiopiat},
Journal = {AMERICAN ECONOMIC REVIEW-INSIGHTS},
Year = {2024},
Volume = {6},
Number = {2},
Pages = {277-294},
Month = {JUN},
Abstract = {We study the long -run effects of a big -push ``graduation{''} program
   in Ethiopia in which very poor households received a one-time transfer
   of productive assets ( mainly livestock ) , technical training , and
   access to savings accounts. After seven years , treatment effects on
   wealth and consumption remain economically meaningful but dissipated
   relative to the two- and three-year results. Treatment effects on other
   outcomes attenuated further. Based on absolute well-being ( e.g. , food
   security ) not dropping , we argue that the treatment effect dissipation
   is driven primarily by improved living standards for control households
   rather than losses of the previously accrued benefits for the treatment
   households.},
