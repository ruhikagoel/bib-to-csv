Title = {MOSIR: Mobile-based Segment and Image Retrieval},
Booktitle = {2011 7TH INTERNATIONAL CONFERENCE ON WIRELESS COMMUNICATIONS, NETWORKING
   AND MOBILE COMPUTING (WICOM)},
Series = {International Conference on Wireless Communications Networking and
   Mobile Computing-WiCOM},
