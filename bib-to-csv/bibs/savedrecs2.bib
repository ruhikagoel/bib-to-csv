@inproceedings{ WOS:000380560600150,
Author = {Clarke, Charles A. and Pfluegel, Eckhard and Tsaptsinos, Dimitris},
Book-Group-Author = {IEEE},
Title = {Hide-as-you-Type: An Approach To Natural Language Steganography Through
   Sentence Modification},
Booktitle = {2014 IEEE INTERNATIONAL CONFERENCE ON HIGH PERFORMANCE COMPUTING AND
   COMMUNICATIONS, 2014 IEEE 6TH INTL SYMP ON CYBERSPACE SAFETY AND
   SECURITY, 2014 IEEE 11TH INTL CONF ON EMBEDDED SOFTWARE AND SYST
   (HPCC,CSS,ICESS)},
Year = {2014},
Pages = {945-952},
Note = {16th IEEE International Conference on High Performance Computing and
   Communications HPCC 2014\textbackslash{}11th IEEE International
   Conference on Embedded Software and Systems ICESS
   2014\textbackslash{}6th International Symposium on Cyberspace Safety and
   Security CSS 2014, Paris, FRANCE, AUG 20-22, 2014},
Organization = {FEMTO-ST Inst; Ecole Cent Paris; Ecole Mines Paris; IEEE; IEEE Computer
   Society; IEEE Tech Comm Scalable Comp},
Abstract = {In this paper, we describe and evaluate Hide-as-you-Type, a prototype
   steganographic system, designed to hide secret bits in the digest chains
   of natural language sentences. Through an interactive embedding process
   that incorporates keyed hash functions and human reviewed sentences, the
   system generates two innocuous natural language carriers that ultimately
   optimise payload secrecy. Conventionally, the first carrier
   (carrier-text) hosts a payload of secret bits. The second carrier,
   referred to as an open-stego-key, hosts sentences used as keys to
   extract a payload from a carrier-text. Notable aspects of our approach
   include the scope to offset embedding distortion from a carrier-text to
   an open-stego-key, as well as the scope to resize a carrier-text for
   optimal transmission via capacity constrained channels (e.g. Twitter,
   SMS), at the expense of a larger open-stego-key, that is transmitted via
   an alternative channel (e.g. social media, blog, forum, email).},
