@article{ WOS:000218895900008,
Author = {Luciano, Carmen and Valdivia-Salas, Sonsoles and Ruiz, Francisco J. and
   Rodriguez-Valverde, Miguel and Barnes-Holmes, Dermot and Dougher,
   Michael J. and Cabello, Francisco and Sanchez, Vanessa and
   Barnes-Holmes, Yvonne and Gutierrez, Olga},
Title = {Extinction of aversive eliciting functions as an analog of exposure to
   conditioned fear: Does it alter avoidance responding?},
Journal = {JOURNAL OF CONTEXTUAL BEHAVIORAL SCIENCE},
Year = {2013},
Volume = {2},
Number = {3-4},
Pages = {120-134},
Month = {OCT 15},
Abstract = {Exposure techniques rely on the assumption that the extinction of the
   classically conditioned response (i.e., fear) will result in the
   disruption of limiting forms of avoidance behavior, both directly
   trained and derived/indirectly established. This report presents
   translational research that attempts to test this assumption in
   laboratory conditions in two experiments with human volunteers. The
   procedure in both experiments included six phases: (1) conditional
   discrimination training for the formation of two 6-member equivalence
   classes; (2) classical conditioning of elicited responses to Class 1
   (Al/B1) and Class 2 (A2/62) members in the white context, followed by
   conditioning of avoidance/approach responses to Class 1/Class 2 members,
   respectively, in the green context; (3) test for the transfer of
   avoidance/ approach functions and of eliciting respondent functions to
   DI /F1 and D2/F2 in the green context; (4) extinction of classically
   conditioned responses to Al /B1 in the white context; (5) test of the
   effects of respondent extinction on avoidance responding to the A, B, D,
   and F stimuli in the green context; and (6) test of derived symmetry and
   equivalence relations. Results show that after successful respondent
   extinction in the white context, only 33.3\% participants stopped
   showing avoidance behavior in the green context, and that respondent
   elicitation was reinstalled during the test (Phase 5). In Phase 4 of
   Experiment 2, in addition to undergoing respondent extinction,
   participants were instructed that the vvhite and green contexts were
   similar. Results show that after successful respondent extinction in the
   vvhite context during Phase 4, only 10\% participants stopped showing
   avoidance behavior in the green context, and that respondent elicitation
   was almost eliminated during the test (Phase 5). We discuss these
   findings and their applied implications. (C) 2013 Association for
   Contextual Behavioral Science. Published by Elsevier Inc. All rights
   reserved.},
