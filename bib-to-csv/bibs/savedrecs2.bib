@article{ WOS:000660636700084,
Author = {Kiss, Sandor Z. and Hosszu, Eva and Tapolcai, Janos and Ronyai, Lajos
   and Rottenstreich, Ori},
Title = {Bloom Filter With a False Positive Free Zone},
